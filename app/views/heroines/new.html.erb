<h1>Create Heroine:</h1>

<%= form_for @heroine do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :super_name %>
    <%= f.text_field :super_name %>
    <br>
    <%= f.label :power%>
    <%= collection_select(:heroine, :power_id, Power.all, :id, :name, prompt: true) %>

    <br>
    <%= f.submit %>
    <% end %>